        <script>
            const
                $boton = document.querySelector("#btnDescargar");
            $imagen = document.querySelector("#codigoqr");

            $boton.onclick = () => {
                const enlace = document.createElement("a");
                enlace.href = $imagen.src;
                enlace.download = "{{ruta}}.png";
                enlace.click();
            }
        </script>

