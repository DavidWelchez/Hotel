</ul>
<!-- THE PANELS -->
<article id="panels">
    <div class="container">
        <section id="panel-1">
            <main>
                <div class="box">

                    <div class="row g-3 needs-validation">

                        <div class="form-group col-md-6">
                            <h5 class="card-title"> Cuotas de Pago: {{inscripcion.cuotasPago}}</h5>
                        </div>
                        <div class="form-group col-md-6">
                            <h5 class="card-title"> Modalidad: {{inscripcion.modalidad}}</h5>
                        </div>

                    </div>
                    <br>
                    <div class="row g-3 needs-validation">
                        <div class="col-md-6">
                            <h5 class="card-title"> Total a pagar: L {{inscripcion.total}}</h5>
                        </div>
                        <div class="col-md-6">
                            <h5 class="card-title"> Fecha de inscripcion: {{inscripcion.created_at}}</h5>
                        </div>

                    <form action="/seguridad/resend-emails" method="post">
                                                            <button class="accordion-button" type="submit" data-bs-toggle="collapse" data-bs-target="#collapseOne"
                                                                aria-expanded="true" aria-controls="collapseOne">Enviar
                                                            </button>
                    </form>
                    </div>
                    <br><br>
                    <div class="table-responsive">
                        <table
                            class="table  table-hover table-hovertable-striped table-class  table-bordered border-light  "
                            id="table-id">
                            <thead>
                                <tr>
                                    <th>Congreso</th>
                                    <th>Hora de inicio</th>
                                    {{#if usuariovirtualcomprobante}}
                                    <th>Transmisión en vivo</th>
                                    {{/if}}

                                </tr>
                            </thead>
                            <tbody>
                                {{#each inscripciondetalle }}
                                <tr>
                                    <td>{{nombre}}</td>
                                    <td>8:00</td>

                                    
                                    {{#if ../usuariovirtualcomprobante}}
                                    {{!-- <td>
                                        <a href="/send-request">
                                            <i class="fas fa-video" style="color: #ff0000"></i> </a>
                                    </td> --}}
                                    {{/if}}


                                </tr>
                                {{else}}
                                <th></th>
                                {{/each}}

                            </tbody>
                        </table>
                        {{#if usuariovirtualcomprobante}}
                        <div>
                            <h7>


                            </h7>
                        </div>

                        {{else }}
                        {{#if usuariovirtual}}


